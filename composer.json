{
    "name": "yourname/laravelglobe",
    "description": "Comprehensive Countries, States, Cities, Currencies, and Timezones for Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your@email.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Yourname\\LaravelGlobe\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yourname\\LaravelGlobe\\LaravelGlobeServiceProvider"
            ],
            "aliases": {
                "Globe": "Yourname\\LaravelGlobe\\Facades\\Globe"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}